package paca_user;

public class UserDAO {
	//필드
	private String name;
	private String id;
	private String pwd;
	private String phone;
	private String email;
	private boolean ph_not;
	private boolean email_not;
	
	private boolean isAdmin;
	
	//생성자
	public UserDAO() {
		
	}

	//getter setter
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getPwd() {
		return pwd;
	}

	public void setPwd(String pwd) {
		this.pwd = pwd;
	}

	public String getPhone() {
		return phone;
	}

	public void setPhone(String phone) {
		this.phone = phone;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public boolean isPh_not() {
		return ph_not;
	}

	public void setPh_not(boolean ph_not) {
		this.ph_not = ph_not;
	}

	public boolean isEmail_not() {
		return email_not;
	}

	public void setEmail_not(boolean email_not) {
		this.email_not = email_not;
	}

	public boolean isAdmin() {
		return isAdmin;
	}

	public void setAdmin(boolean isAdmin) {
		this.isAdmin = isAdmin;
	}
	
	
	
}
